<odoo>
    
    <record id="type_document_view_tree" model="ir.ui.view">
        <field name="name">type_document_view_tree</field>
        <field name="model">esd.type.document</field>
        <field name="arch" type="xml">
            <tree string="Type Document" editable="top">
                <field name="name"/>
                <field name="is_individual"/>
                <field name="is_company"/>
            </tree>
        </field>
    </record>
    
    <record id="type_document_action" model="ir.actions.act_window">
        <field name="name">Type of Document</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">esd.type.document</field>
        <field name="view_mode">tree</field>
    </record>
    
</odoo>