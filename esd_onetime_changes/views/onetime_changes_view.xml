<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="onetime_changes_view_form_inherit" model="ir.ui.view">
            <field name="name">Novedades</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <group name="payroll_group" position="after">
                    <separator string="Novedades"/>
                    <field name="hr_newsness">
                        <tree editable="bottom">
                            <field name="name"/>
                            <field name="amount"/>
                            <field name="start_date"/>
                            <field name="end_date"/>
                            <field name="pdo"/>
                            <field name="state_newness"/>
                        </tree>
                    </field>
                </group>
            </field>
        </record>
    </data>
</odoo>
