<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="interest_rate_loans" model="ir.ui.view">
        <field name="name">interest.rate</field>
        <field name="model">interest.rate</field>
        <field name="arch" type="xml">
            <tree string="Interest Rate" editable="bottom">
                <field name="start_quota"/>
                <field name="end_quota"/>
                <field name="interest_rate"/>
            </tree>
        </field>
    </record>

    <record id="action_interest_rate" model="ir.actions.act_window">
        <field name="name">Interest Rate</field>
        <field name="res_model">interest.rate</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem name="Interest Rate"
              id="menu_interest_rate"
              sequence="4"
              action="action_interest_rate"
              parent="loans.configuration_menu"/>
</odoo>
